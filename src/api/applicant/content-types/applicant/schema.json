      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "applicants"
    },
    "credentials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::credential.credential"
    },
    "program": {
      "type": "enumeration",
      "enum": [
        "gradeSchool",
        "juniorHighSchool",
        "seniorHighSchool"
      ]
    },
    "firstName": {
      "type": "string"
    },
    "middleName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "gradeLevel": {
      "type": "string"
    },
    "profilePicture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    }
  }
