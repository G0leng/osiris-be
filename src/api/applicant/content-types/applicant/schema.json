{
  "kind": "collectionType",
  "collectionName": "applicants",
  "info": {
    "singularName": "applicant",
    "pluralName": "applicants",
    "displayName": "applicant",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "institution": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::institution.institution"
    },
    "branch": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::branch.branch"
    },
    "exams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam.exam",
      "mappedBy": "applicant"
    },
    "interviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::interview.interview",
      "mappedBy": "applicant"
    },
    "interviewNotes": {
      "type": "text"
    },
    "admissionStatus": {
      "type": "boolean",
      "default": false
    },
