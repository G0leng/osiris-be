{
  "kind": "collectionType",
  "collectionName": "payments",
  "info": {
    "singularName": "payment",
    "pluralName": "payments",
    "displayName": "payment",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "accounts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::account.account"
    },
    "paymentDate": {
      "type": "datetime"
    },
    "transactionType": {
      "type": "enumeration",
      "enum": [
        "cash",
        "card",
        "cheque",
        "bank"
      ]
    },
    "cashier": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "student": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::student.student",
      "inversedBy": "payments"
    },
    "amount": {
      "type": "integer"
    },
    "paymentProof": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
